#include "quatconj.h"

//!  conjugate of a quaternion
/*
 \param Q quaternion to take conjugate of
 \param QResult Result of conjugation
 */
void quatconj(const quaternion* Q, quaternion* QResult)
{
  QResult->w = Q->w;
  QResult->x = -Q->x;
  QResult->y = -Q->y;
  QResult->z = -Q->z;

}
